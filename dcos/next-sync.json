{
   "id":"/qtask-nextcloud-resilio",
   "containers":[
      {
         "name":"nextcloud-pods",
         "resources":{
            "cpus":1,
            "mem":512,
            "disk":0,
            "gpus":0
         },
         "image":{
            "kind":"DOCKER",
            "id":"nexcloud"
         },
         "endpoints":[
            {
               "name":"web-nextcloud",
               "containerPort":80,
               "protocol":[
                  "http"
               ]
            }
         ],
         "volumeMounts":[
            {
               "name":"sharedvolume",
               "mountPath":"data/"
            }
         ]
      },
      {
         "name":"resilio-pods",
         "resources":{
            "cpus":1,
            "mem":128,
            "disk":0,
            "gpus":0
         },
         "image":{
            "kind":"DOCKER",
            "id":"qtask/sync-docker"
         },
         "endpoints":[
            {
               "name":"web-resilio",
               "containerPort":8888,
               "protocol":[
                  "http"
               ]
            }
         ],
         "volumeMounts":[
            {
               "name":"sharedvolume",
               "mountPath":"data"
            }
         ]
      }
   ],
   "networks":[
      {
         "mode":"container"
      }
   ],
   "volumes":[
      {
         "name":"sharedvolume"
      }
   ]
}
